{
  "manifest_version": 2,
  "name": "JSON Viewer 2",
  "version": "1.0",
  "description": "Format the JSON in the browser",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "browser-polyfill.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "*.svg",
    "*.ttf",
    "*.woff",
    "*.woff2",
    "*.eot"
  ]
}