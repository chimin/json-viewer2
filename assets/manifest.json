{
  "manifest_version": 3,
  "name": "Pokok JSON Viewer",
  "version": "1.8.1",
  "description": "Show the JSON in tree view or table view",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "browser-polyfill.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "*.svg",
    "*.ttf",
    "*.woff",
    "*.woff2",
    "*.eot"
  ]
}